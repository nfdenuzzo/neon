{
  "description": "A component for displaying notification counts to the user.",
  "tags": {},
  "displayName": "NeonNotificationCounter",
  "exportName": "default",
  "props": [
    {
      "name": "active",
      "tags": {},
      "description": "Whether or not the notification is active",
      "type": {
        "name": "boolean"
      },
      "defaultValue": {
        "func": false,
        "value": "false"
      }
    },
    {
      "name": "color",
      "tags": {},
      "description": "The notification color",
      "type": {
        "name": "NeonFunctionalColor"
      },
      "defaultValue": {
        "func": false,
        "value": "NeonFunctionalColor.Error"
      }
    },
    {
      "name": "count",
      "tags": {},
